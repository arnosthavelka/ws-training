<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
      xmlns:xs="http://www.w3.org/2001/XMLSchema"
      xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
      xmlns="urn:com:asseco:aha:training:ws:v1" 
      xmlns:dto="urn:com:asseco:aha:training:types:v1" 
      targetNamespace="urn:com:asseco:aha:training:ws:v1"
      name="EndpointAPI">

	<wsdl:documentation>Version 0.0.1</wsdl:documentation>

	<wsdl:types>
		<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:import namespace="urn:com:asseco:aha:training:types:v1" schemaLocation="../xsd/ws-training-types.xsd" />
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="EndpointRequest">
		<wsdl:part element="dto:EndpointRequest" name="request" />
	</wsdl:message>

	<wsdl:portType name="EndpointPortType">
		<jaxws:bindings xmlns:jaxws="http://java.sun.com/xml/ns/jaxws">
			<jaxws:enableWrapperStyle>false</jaxws:enableWrapperStyle>
		</jaxws:bindings>
		<wsdl:operation name="receive">
			<wsdl:input message="EndpointRequest" name="EndpointRequest"/>
		</wsdl:operation>
	</wsdl:portType>
	
	<wsdl:binding name="EndpointBinding" type="EndpointPortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="receive">
			<soap:operation soapAction="receive" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
		</wsdl:operation>
	</wsdl:binding>
		
	<wsdl:service name="EndpointService">
		<wsdl:port binding="EndpointBinding" name="EndpointPort">
			<soap:address location="???" />
		</wsdl:port>
	</wsdl:service>
	
</wsdl:definitions>