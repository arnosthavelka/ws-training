<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="urn:com:asseco:aha:training:types:v1"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:xmime="http://www.w3.org/2005/05/xmlmime"
           targetNamespace="urn:com:asseco:aha:training:types:v1"
           elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.0.1">

	<xs:element name="AddRequest" type="AddRequest"/>
	<xs:element name="AddResponse" type="AddResponse"/>
	
	<xs:complexType name="AddRequest">
		<xs:sequence>
			<xs:element name="number" type="xs:int" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="AddResponse">
		<xs:sequence>
			<xs:element name="result" type="xs:int"/>
		</xs:sequence>
	</xs:complexType>
	
</xs:schema>
